\n        token: JSON.parse(localStorage.getItem('token')).token,\r\n      });\r\n    } else if (token && token.login === true) {\r\n      this.setState({\r\n        login: true,\r\n        token: JSON.parse(localStorage.getItem('token')).token,\r\n      });\r\n    } else {\r\n      this.setState({ login: false, admin: false });\r\n    }\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        {this.state.admin === true ? (\r\n          <AdminPanel login={this.state.login} token={this.state.token} />\r\n        ) : (\r\n            <Customers login={this.state.login} token={this.state.token} />\r\n          )}\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Home;\r\n",["164","165"],"C:\\Users\\David\\Desktop\\finalProject\\client\\src\\components\\layout\\Nav.js",["166"],"import React, { Component } from 'react';\r\nimport {\r\n  BrowserRouter as Router,\r\n  Link,\r\n  Redirect,\r\n} from \"react-router-dom\";\r\nimport { Grid, Button, Typography } from '@material-ui/core';\r\n\r\nexport class Nav extends Component {\r\n  constructor() {\r\n    super();\r\n    this.state = {\r\n      logout: false,\r\n    }\r\n  }\r\n  handleLogout = () => {\r\n    this.setState(previousState => {\r\n      return {\r\n        logout: !previousState.logout\r\n      }\r\n    })\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <Grid container justify=\"center\" spacing={6}>\r\n        <Grid container item md={4} justify=\"center\">\r\n          <Grid item>\r\n            <Typography variant=\"h2\">Restaurant Reviews Demo </Typography>\r\n          </Grid>\r\n        </Grid>\r\n        <Grid container item md={8} alignItems=\"center\" direction=\"row\" justify=\"center\" spacing={4}>\r\n          <Grid item >\r\n            <Link to={'/'}>\r\n              <Button variant=\"contained\" color=\"primary\">\r\n                Home\r\n              </Button>\r\n            </Link>\r\n          </Grid>\r\n          <Grid item >\r\n            <Link to={'/signup'}>